{
  "name": "vleap/core",
  "description": "vLeap Core for PHP",
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Vleap\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vleap\\Core\\Tests\\": "tests"
    }
  },
  "authors": [],
  "require": {
    "php": "^8.1",
    "illuminate/collections": "^10.0 || ^11.0",
    "nesbot/carbon": "^2.66 || ^3.2"
  },
  "require-dev": {
    "pestphp/pest": "^2.25"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
